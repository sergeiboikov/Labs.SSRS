  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdventureWorksDW2012</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT dpc.EnglishProductCategoryName,
dps.EnglishProductSubcategoryName,
dp.EnglishProductName,
dst.SalesTerritoryRegion,
dst.SalesTerritoryCountry,
dst.SalesTerritoryGroup,
fis.SalesAmount
FROM DimProduct dp
INNER JOIN DimProductSubcategory dps ON dp.ProductSubcategoryKey = dps.ProductSubcategoryKey 
INNER JOIN DimProductCategory dpc ON dps.ProductCategoryKey = dpc.ProductCategoryKey 
INNER JOIN FactInternetSales fis ON dp.ProductKey = fis.ProductKey 
INNER JOIN DimSalesTerritory dst ON fis.SalesTerritoryKey = dst.SalesTerritoryKey
WHERE (fis.OrderDateKey / 10000 = @Year)
AND ((fis.OrderDateKey / 100 % 100) IN (@Month))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="EnglishProductCategoryName">
          <DataField>EnglishProductCategoryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EnglishProductSubcategoryName">
          <DataField>EnglishProductSubcategoryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EnglishProductName">
          <DataField>EnglishProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesTerritoryRegion">
          <DataField>SalesTerritoryRegion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesTerritoryCountry">
          <DataField>SalesTerritoryCountry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesTerritoryGroup">
          <DataField>SalesTerritoryGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesAmount">
          <DataField>SalesAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Years">
      <Query>
        <DataSourceName>AdventureWorksDW2012</DataSourceName>
        <CommandText>SELECT distinct (OrderDateKey/10000) as [Year]
FROM FactInternetSales</CommandText>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Months">
      <Query>
        <DataSourceName>AdventureWorksDW2012</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT DATENAME(MONTH,convert(date,convert(varchar(8),OrderDateKey),112)) as [MonthName]
,DATEPART(MONTH,convert(date,convert(varchar(8),OrderDateKey),
112)) as [MonthKey]
FROM FactInternetSales
where OrderDateKey/10000=@Year
order by DATEPART(MONTH,convert(date,convert
(varchar(8),OrderDateKey),
112))</CommandText>
      </Query>
      <Fields>
        <Field Name="MonthName">
          <DataField>MonthName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MonthKey">
          <DataField>MonthKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
